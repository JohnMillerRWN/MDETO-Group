<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="456.0" prefWidth="248.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BOE.view.ProjectListController">
   <children>
      <TableView fx:id="project_list" prefHeight="418.0" prefWidth="248.0">
         <columns>
            <TableColumn fx:id="idCol" prefWidth="48.0" text="ID" />
            <TableColumn fx:id="nameCol" prefWidth="151.0" text="Project Name" />
         </columns>
      </TableView>
      <ButtonBar layoutY="416.0" AnchorPane.bottomAnchor="6.0" AnchorPane.rightAnchor="10.0">
        <buttons>
          <Button fx:id="loadButton" mnemonicParsing="false" onAction="#getProjectFromList" text="Load" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
